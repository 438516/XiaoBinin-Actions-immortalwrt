
config smartdns
	option server_name 'smartdns'
	option port '6053'
	option auto_set_dnsmasq '0'
	option ipv6_server '1'
	option dualstack_ip_selection '1'
	option serve_expired '1'
	option cache_persist '1'
	option resolve_local_hostnames '1'
	option rr_ttl_min '600'
	option seconddns_port '6553'
	option tcp_server '0'
	option prefetch_domain '1'
	option cache_size '500000'
	option force_https_soa '0'
	option bind_device '0'
	option seconddns_enabled '1'
	option seconddns_tcp_server '0'
	option seconddns_server_group 'oceans'
	option seconddns_no_rule_addr '1'
	option seconddns_no_rule_nameserver '1'
	option seconddns_no_rule_ipset '1'
	option seconddns_no_rule_soa '1'
	option seconddns_no_dualstack_selection '1'
	option seconddns_no_cache '1'
	option seconddns_force_aaaa_soa '1'
	option old_port '6053'
	option old_enabled '0'
	option old_auto_set_dnsmasq '0'

config domain-rule
	option server_group 'oceans'
	option no_speed_check '1'
	option force_aaaa_soa '1'

config server
	option ip 'https://8.8.4.4/dns-query'
	option type 'https'
	option server_group 'oceans'
	option blacklist_ip '0'
	option no_check_certificate '0'
	option exclude_default_group '1'
	option name 'Google DNS'
	option enabled '1'

